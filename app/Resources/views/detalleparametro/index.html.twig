{% extends 'base.html.twig' %}

{% block body %}
    <h1>DetalleParametro list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Valor</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for detalleParametro in detalleParametros %}
            <tr>
                <td><a href="{{ path('admin_detalleparametro_show', { 'id': detalleParametro.id }) }}">{{ detalleParametro.id }}</a></td>
                <td>{{ detalleParametro.nombre }}</td>
                <td>{{ detalleParametro.valor }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('admin_detalleparametro_show', { 'id': detalleParametro.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('admin_detalleparametro_edit', { 'id': detalleParametro.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_detalleparametro_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
