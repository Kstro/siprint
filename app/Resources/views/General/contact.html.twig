{% extends ':Layout:general.html.twig' %}

{% block section %}
    Contact us
{% endblock %}

{% block section2 %}
    Contact us
{% endblock %}

{% block contenido %}
    <section id="pr-contact" class="pr-main">
            <div class="container"><h1 class="ct-header">Contact us</h1></div>
            <div class="contact-map">
            <!--<img src="images/maps.jpg" />-->
                    {#<iframe width="100%" height="500" frameborder="0" src="https://maps.google.com/maps?f=q&amp;source=s_q&amp;hl=vi&amp;geocode=&amp;q=Avenue+of+the+Americas,+New+York,+Ti%E1%BB%83u+bang+New+York,+Hoa+K%E1%BB%B3&amp;aq=0&amp;oq=new+york+Av&amp;sll=10.879183,106.799662&amp;sspn=0.012833,0.021136&amp;gl=VN&amp;ie=UTF8&amp;hq=&amp;hnear=Avenue+of+the+Americas,+New+York,+Hoa+K%E1%BB%B3&amp;t=m&amp;z=14&amp;ll=40.76054,-73.979975&amp;output=embed" marginwidth="0" marginheight="0" scrolling="no"></iframe>		#}
                    <iframe src="https://www.google.com/maps/embed?pb=!1m19!1m8!1m3!1d6614.244408704089!2d-118.25884499999998!3d34.015074!3m2!1i1024!2i768!4f13.1!4m8!3e0!4m5!1s0x80c2c87ce9548b13%3A0xb2a9a8f8cff152fc!2s3003+S+Central+Ave%2C+Los+Angeles%2C+CA+90011%2C+EE.+UU.!3m2!1d34.015069499999996!2d-118.25665629999999!4m0!5e0!3m2!1ses!2ssv!4v1453215112266" width="100%" height="500" frameborder="0"></iframe>
            </div>

            <div class="container">
                <div class="col-md-3 col-sm-3 col-xs-12">
                    <div class="address">
                        <i class="fa fa-home"></i>
                        <p>
                        <span>Address: 3003 S Central Ave</span><br/>
                        <span>City: Los Angeles</span><br/>
                        <span>State: California </span><br/>
                        <span>Zip Code: 90011 </span><br/>
                        <span>Country: USA </span>
                        </p>
                    </div>
                    <div class="phone">
                        <i class="fa fa-phone"></i>
                        <p>
                        <span>Telephone: +323 3781554</span>						
                        </p>
                    </div>

                    {#<div class="fax">
                        <i class="fa fa-fax"></i>
                        <p>
                        <span>Fax: +321 5441601 </span>						
                        </p>
                    </div>#}

                    <div class="website">
                        <i class="fa fa-globe"></i>
                        <p>
                        <span>Website: expressionsprint.com </span>						
                        </p>
                    </div>
                </div>

                {{ form_start(form, {'action': path('dg_general_contact')}, {'attr': {'class':'form-validate form-horizontal'} }) }}
                {{ form_errors(form) }}
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        {{ form_widget(form.mensaje, {'attr': {'class':'required invalid', 'rows': '10', 'cols': '50', 'placeholder': 'Message *'} }) }}
                        <p>Ask us a question and we'll write back to you promptly! Simply fill out the form below and click Send Email.</p>
                        <p>Thanks. Items marked with an asterisk (<span class="star">*</span>) are required fields.</p>
                    </div>

                    <div class="col-md-3 col-sm-3 col-xs-12">
                        {{ form_widget(form.nombre, {'attr': {'class':'name form-control', 'placeholder': 'Enter your name *', 'style': 'margin-bottom: 30px' } }) }}
                        {{ form_widget(form.correoElectronico, {'attr': {'class':'mesage form-control', 'placeholder': 'Enter E-mail *', 'style': 'margin-bottom: 30px'} }) }}
                        {{ form_widget(form.tema, {'attr': {'class':'mesage form-control', 'placeholder': 'Enter Mesage Subject *', 'style': 'margin-bottom: 30px'} }) }}

                        <input type="submit" value="Submit" style="margin-bottom: 10px;" class="sendmail btn btn-primary form-control"/>
                    </div>
                {{ form_end(form) }}
                {#</form>#}
            </div>
    </section>
{% endblock %}    
{% block javascript_step %}
    <script>
        $(document).ready(function(){
            $('#suscripcion_nombre').val('');
            $('#suscripcion_correoElectronico').val('');
            $('#suscripcion_tema').val('');
        });
    </script>
{% endblock %}